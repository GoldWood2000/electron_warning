{
  "name": "cloud-doc",
  "version": "0.1.0",
  "main": "./src/main.js",
  "private": true,
  "author": {
    "name": "leo",
    "email": "2058386844@qq.com",
    "url": "https://github.com/GoldWood2000"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "electron-is-dev": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "wait-on": "^6.0.1",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-builder": "^23.1.0",
    "electron": "^19.0.10"
  },
  "homepage": "./",
  "build": {
    "appId": "CLOUDDOC",
    "productName": "测试Eletron打包",
    "copyright": "Copyright © year ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json",
      "./src/main.js"
    ],
    "extends": null,
    "mac": {
      "icon": "./public/react_ts.png"
    },
    "win": {}
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "electron .",
    "serve": "concurrently \"wait-on http://localhost:3000 && electron .\" \" cross-env BROWSER=none npm start\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "prepack": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}